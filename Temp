#!/bin/bash

# Simulate the lmstat output
lmstat=$(cat <<EOF
fllv28.itg.com: license server UP (MASTER) v11.19.0

Vendor daemon status (on fllv28.itg.com):

ansyslmd: UP v11.19.0
ansoftd: UP v11.19.0
EOF
)

# Extract the first vendor daemon status
vendor_status=$(echo "$lmstat" | grep -m 1 -o '^[^:]*: UP' | awk '{print $1}')

# Print the result
echo "First vendor daemon status: $vendor_status"
