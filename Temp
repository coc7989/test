UPDATE lsf
SET SB = CASE
    WHEN SLA LIKE 'APP%' THEN 'Analog Power Products'
    WHEN SLA LIKE 'ASC%' THEN 'Analog Signal Chain'
    WHEN SLA LIKE 'EP%' OR SLA LIKE 'EMBEDDED_PROCESSORS%' OR SLA LIKE 'RADAR%' OR SLA LIKE 'SITARA%' THEN 'Embedded Processors'
    WHEN SLA LIKE 'OTHERS%' THEN 'Others'
    ELSE NULL
END;
