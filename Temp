from flask import Flask, jsonify

app = Flask(__name__)

# Define the API URL prefix
apiurlprefix = "/api/v1"

# Example routes using the prefix
@app.route(f'{apiurlprefix}/example', methods=['GET'])
def example():
    return jsonify(message="This is an example API call")

@app.route(f'{apiurlprefix}/another', methods=['GET'])
def another_example():
    return jsonify(message="This is another API call")

if __name__ == '__main__':
    app.run(debug=True)
