pipeline {
    agent any

    stages {
        stage('Disable Crontab') {
            steps {
                script {
                    sh 'sed -i "/check_daemon_prs/s/^/#/" filename' // Comment out the first cron entry
                    sh 'sed -i "/newlog/s/^/#/" filename' // Comment out the second cron entry
                    sh 'sed -i "/rlmonitor/s/^/#/" filename' // Comment out the third cron entry
                }
            }
        }
        stage('Perform Actions') {
            steps {
                // Your actions here
            }
        }
        stage('Enable Crontab') {
            steps {
                script {
                    sh 'sed -i "/check_daemon_prs/s/^#//" filename' // Uncomment the first cron entry
                    sh 'sed -i "/newlog/s/^#//" filename' // Uncomment the second cron entry
                    sh 'sed -i "/rlmonitor/s/^#//" filename' // Uncomment the third cron entry
                }
            }
        }
    }
}
