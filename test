output=$(/home/flames/vendors/cds/pgms.linux/lmstat -c /home/flames/data/\$mappedlicense)

# Check if the output contains both "lmreread successful" and "lmreread- Copyright 1980"
if [[ $output == *"lmreread successful"* && $output == *"lmreread- Copyright 1980"* ]]; then
    echo "lmreread was successful"
else
    echo "lmreread failed or not successful"
fi
