#!/bin/bash

# Run the command and store the output in the verification variable
verification=$(sssverify "$mappedlicense")

# Check if the output contains the string "License file Integrity check FAILED!"
verification_status=$(echo "$verification" | grep "License file Integrity check FAILED!")

# Conditional check
if [ "$verification_status" != "License file Integrity check FAILED!" ]; then
  echo "success"
else
  echo "License file Integrity check FAILED!"
fi
