def job_ids = "/661033,663858/"

def job_ids_pattern = job_ids.split(',').join(',')

sh(script: "/data/lsf/work/local/bin/mbjobs2 | tee output_file.txt", returnStdout: true)

sh(script: "grep PEND output_file.txt > filtered_output.txt")

sh(script: "cut -d',' -f 2,15,16,17,23 filtered_output.txt > cut_output.txt")

sh(script: "grep -E '${job_ids_pattern}' cut_output.txt > final_output.txt")

def output = readFile('final_output.txt').trim()
