import cx_Oracle

# Establish connection to your Oracle database
connection = cx_Oracle.connect("username", "password", "hostname:port/service_name")

# Create a cursor
cursor = connection.cursor()

# Define your variables holding the values
vendor_name = "your_vendor_name"
server_name = "your_server_name"

# Execute the delete operation with parameterized query
sql = "DELETE FROM your_table_name WHERE vendor_name = :vendor_name AND server_name = :server_name"
cursor.execute(sql, (vendor_name, server_name))

# Commit the transaction
connection.commit()

# Close cursor and connection
cursor.close()
connection.close()
